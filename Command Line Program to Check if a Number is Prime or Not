#include <stdio.h>
#define max 9999
char prime(int num)
{
    int i,j;
    int flag;
    for(i=2;i<num;i++)
    {
        flag=0;
        if(num%i ==0)
        {
            return('n');
            break;
        }
        
    }
    if(flag==0)
    {
        return('y');
    }
}


int main()
{
    int num=11;
    printf("%c",prime(num));
    
    return 0;
}
